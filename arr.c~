#include <stdio.h>
int main()
{
    
    int arr[] = {3, 4, 5};
    int size = 3;
    int i, j = 0, pro = 1;
    for(i = 0; i < size; i++)
    {
           
         pro = pro * arr[i]; 
        
    }
    
    for(i = 0; i < size; i++)
    {
        
        arr[i] = pro/arr[i];
        printf("%d\n", arr[i]);
    }

    

  
    return 0;
}

